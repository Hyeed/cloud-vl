{
  "name": "code-vl",
  "version": "0.0.1",
  "description": "Projet cours Cloud et programmation de VL.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "fix": "npm run lint && npm run format",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "start": "node dist/index.js",
    "test": "jest --config .jestrc.json",
    "test:coverage": "jest --config .jestrc.json --coverage",
    "watch": "nodemon --config .nodemon.json src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hyeed/cloud-vl.git"
  },
  "author": "Henri Lardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hyeed/cloud-vl/issues"
  },
  "homepage": "https://github.com/Hyeed/cloud-vl",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.6",
    "@types/node": "^18.18.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-jest": "^27.4.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
